{
	"_name": <%= name %>,
	"renderable": "airlock",
	"transform": {
		<% if (locals.rotate) { %>"rotate": <%= rotate %>,<% } %>
		"translate": <%= translate %>
	},
	"scene_connection": {},
	"physics": {
		"model": "airlock",
		"dynamic": false
	}
},
<%- include('slide_door', {
	name: name + '-out',
	translate: [0, 0, 0],
	rotate: 0,
	input_signal: locals.out_signal,
	parent: name
}) %>,
<%- include('slide_door', {
	name: name + '-in',
	translate: [3, 0, 0],
	rotate: 0,
	input_signal: locals.in_signal,
	parent: name
}) %>,
{
	"_name": <%= name + '-light' %>,
	"light": {
		"tint": [0.5, 0.5, 1],
		"intensity": 10,
		"spotAngle": 60,
		"shadowMapSize": 11,
		"shadowMapClip": [0.1, 70]
	},
	"transform": {
		"parent": <%= name %>,
		"rotate": [-90, 1, 0, 0],
		"translate": [1.5, 3, 0]
	}
}
