{
	"_name": <%= name + '-root' %>,
	"transform": {
		<% if (locals.parent) { %>"parent": <%= parent %>,<% } %>
		<% if (locals.rotate) { %>"rotate": <%= rotate %>,<% } %>
		"translate": <%= translate %>
	},
	"renderable": "door-frame",
	"physics": {
		"model": "door-frame",
		"dynamic": false
	}
},
{
	"_name": <%= name + '-left' %>,
	"renderable": "door-panel",
	"transform": {
		"parent": <%= name + '-root' %>
	},
	"animation": {
		"states": [
			{"delay": 0.5},
			{
				"delay": 0.5,
				"translate": [0, 0, 1.4]
			}
		]
	},
	"physics": {
		"model": "door-panel",
		"dynamic": false
	}
},
{
	"_name": <%= name + '-right' %>,
	"renderable": "door-panel",
	"transform": {
		"parent": <%= name + '-root' %>,
		"rotate": [180, 0, 1, 0]
	},
	"animation": {
		"states": [
			{
				"delay": 0.5,
				"rotate": [180, 0, 1, 0]
			},
			{
				"delay": 0.5,
				"translate": [0, 0, -1.4],
				"rotate": [180, 0, 1, 0]
			}
		]
	},
	"physics": {
		"model": "door-panel",
		"dynamic": false
	}
},
{
	"_name": <%= name %>,
	"slideDoor": {
		"left": <%= name + '-left' %>,
		"right": <%= name + '-right' %>
	},
	"signalReceiver": {}
}
