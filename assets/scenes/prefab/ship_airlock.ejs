<% if (!locals.scene && !name.startsWith("global:")) {
	name = "global:" + name;
} %>
{
	"name": <%= name %>,
	"transform":  <%= transform %>,
	<% if (locals.load_signal) { %>
	"signal_bindings": {
		"load_scene_connection": <%= load_signal %>
	},
	<% } %>
	"scene_connection": <%= locals.scene_connection || [] %>
},
{
	"name": <%= name + "-zone" %>,
	"transform": {
		"parent": <%= name %>,
		"translate": [0, 2, 0],
		"scale": [4, 4.2, 4]
	},
	"script": {
		"prefab": "template",
		"parameters": { "source": "laser_cube" }
	},
	"signal_output": {},
	"trigger_area": {},
	"event_bindings": {
		<% if (locals.trigger_command) { %>
		"/trigger/player/enter": {
			"target": "console:input/action/run_command",
			"set_value": <%= trigger_command %>
		}
		<% } %>
	}
},
{
	"name": <%= name + '-in' %>,
	"transform": {
		"parent": <%= name %>,
		"translate": [0, 0, -1.95],
		"rotate": [90, 0, 1, 0],
		"scale": [1, 1.5, 2.3]
	},
	"script": {
		"prefab": "template",
		"parameters": { "source": "slide_door" }
	}
},
{
	"name": <%= name + '-out' %>,
	"transform": {
		"parent": <%= name %>,
		"translate": [0, 0, 2],
		"rotate": [90, 0, 1, 0],
		"scale": [1, 1.5, 2.3]
	},
	"script": {
		"prefab": "template",
		"parameters": { "source": "slide_door" }
	}
},
{
	"name": <%= name + '-button-out' %>,
	"transform": {
		"parent": <%= name %>,
		"translate": [1.95, 1.1, 1]
	},
	"script": {
		"prefab": "template",
		"parameters": {
			"source": "button"
		}
	}
},
{
	"name": <%= name + '-button-out.button' %>,
	"event_bindings": {
		"/button/press": [
			{
				"target": "console:input/action/run_command",
				"set_value": <%= "togglesignal " + name + "-out/door_open; wait 300 togglesignal station-center:elevator_door/door_open" %>
			}
		]
	}
},
{
	"name": <%= name + "-floor" %>,
	"renderable": "box",
	"transform": {
		"parent": <%= name %>,
		"translate": [0, -0.25, 0],
		"scale": [4, 0.5, 4.15]
	},
	"physics": {
		"model": "box",
		"dynamic": false
	}
},
{
	"name": <%= name + "-left-wall" %>,
	"renderable": "box",
	"transform": {
		"parent": <%= name %>,
		"translate": [2.2, 2, 0],
		"scale": [0.5, 4.1, 4]
	},
	"physics": {
		"model": "box",
		"dynamic": false
	}
},
{
	"name": <%= name + "-right-wall" %>,
	"renderable": "box",
	"transform": {
		"parent": <%= name %>,
		"translate": [-2.2, 2, 0],
		"scale": [0.5, 4.1, 4]
	},
	"physics": {
		"model": "box",
		"dynamic": false
	}
},
{
	"name": <%= name + "-ceiling" %>,
	"renderable": "box",
	"transform": {
		"parent": <%= name %>,
		"translate": [0, 4, 0],
		"scale": [4, 0.1, 4]
	},
	"physics": {
		"model": "box",
		"dynamic": false
	}
},
{
	"name": <%= name + '-light' %>,
	"transform": {
		"parent": <%= name %>,
		"translate": [0, 3.95, 0],
		"rotate": [[270, 1, 0, 0]]
	},
	"light": {
		"tint": [0.5, 0.5, 1],
		"intensity": 300,
		"spotAngle": 60
	},
	"script": {
		"prefab": "template",
		"parameters": { "source": "spotlight" }
	}
}
