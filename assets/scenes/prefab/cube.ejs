{
	"_name": <%= name %>,
	"renderable": "mcube",
	"transform": {
		<% if (locals.rotate) { %>"rotate": <%= rotate %>,<% } %>
		"translate": <%= translate %>
	},
	"physics": {
		"model": "mcube",
		"altModel": "mcube-alt",
		"density": 4
	}
}
<%
faces = [
	[0, -0.5],
	[-0.5, 0],
	[0, 0.5],
	[0.5, 0]
];
for (i = 0; i < locals.sides; i++) { %>
,
{
	"renderable": "mirror",
	"mirror": [1, 1],
	"transform": {
		"parent": <%= name %>,
		"rotate": [<%= i * 90 %>, 0, 1, 0],
		"translate": [<%= faces[i][0] %>, 0.5, <%= faces[i][1] %>]
	}
},
{
	"renderable": "occluder",
	"transform": {
		"parent": <%= name %>,
		"rotate": [<%= i * 90 + 180 %>, 0, 1, 0],
		"translate": [<%= faces[i][0] * 0.95 %>, 0.5, <%= faces[i][1] * 0.95 %>]
	}
}
<% } %>
