{
    "input:mouse": {
        "event_bindings": {
            "/mouse/scroll": [
                {
                    "outputs": "player:flatview/action/menu/scroll",
                    "filter": "is_primary_focus(HUD)"
                },
                {
                    "outputs": "gui:menu/action/menu/scroll",
                    "filter": "is_primary_focus(Menu)"
                },
                {
                    "outputs": "gui:overlay/action/menu/scroll",
                    "filter": "is_primary_focus(Overlay)"
                }
            ],
            "/mouse/move": [
                {
                    "outputs": "player:flathead/script/camera_rotate",
                    "filter": "is_primary_focus(Game) && !player:pointer/interact_rotate",
                    "modify": ["event.x * 0.001", "event.y * 0.001"]
                },
                {
                    "outputs": "player:flathead/camera/rotate",
                    "filter": "is_primary_focus(Game) && !player:pointer/interact_rotate",
                    "modify": ["event.x * 0.001", "event.y * 0.001"]
                },
                {
                    "outputs": "player:pointer/interact/rotate",
                    "filter": "is_primary_focus(Game)",
                    "modify": ["event.x * 0.01", "event.y * 0.01"]
                }
            ],
            "/mouse/pos": [
                {
                    "outputs": "player:flatview/action/menu/cursor",
                    "filter": "is_primary_focus(HUD)"
                },
                {
                    "outputs": "gui:menu/action/menu/cursor",
                    "filter": "is_primary_focus(Menu)"
                },
                {
                    "outputs": "gui:overlay/action/menu/cursor",
                    "filter": "is_primary_focus(Overlay)"
                }
            ],
            "/mouse/left_click": [
                {
                    "outputs": "player:pointer/interact/press",
                    "filter": "is_primary_focus(Game)"
                },
                {
                    "outputs": "player:flatview/action/menu/primary_trigger",
                    "filter": "is_primary_focus(HUD)"
                },
                {
                    "outputs": "gui:menu/action/menu/primary_trigger",
                    "filter": "is_primary_focus(Menu)"
                },
                {
                    "outputs": "gui:overlay/action/menu/primary_trigger",
                    "filter": "is_primary_focus(Overlay)"
                }
            ],
            "/mouse/right_click": [
                {
                    "outputs": "player:flatview/action/menu/secondary_trigger",
                    "filter": "is_primary_focus(HUD)"
                },
                {
                    "outputs": "gui:menu/action/menu/secondary_trigger",
                    "filter": "is_primary_focus(Menu)"
                },
                {
                    "outputs": "gui:overlay/action/menu/secondary_trigger",
                    "filter": "is_primary_focus(Overlay)"
                }
            ]
        }
    },
    "input:keyboard": {
        "event_bindings": {
            "/keyboard/key/f1": {
                "outputs": "console:input/action/run_command",
                "filter": "event",
                "set_value": "edit"
            },
            "/keyboard/key/f2": {
                "outputs": "console:input/action/run_command",
                "filter": "event",
                "set_value": "tracy"
            },
            "/keyboard/key/f5": {
                "outputs": "console:input/action/run_command",
                "filter": "event",
                "set_value": "reloadscene"
            },
            "/keyboard/key/f6": {
                "outputs": "console:input/action/run_command",
                "filter": "event",
                "set_value": "reloadplayer; reloadscene; respawn"
            },
            "/keyboard/key/f7": {
                "outputs": "console:input/action/run_command",
                "filter": "event",
                "set_value": "reloadshaders; reloadscripts"
            },
            "/keyboard/key/backtick": {
                "outputs": "gui:overlay/action/console/toggle",
                "filter": "event"
            },
            "/keyboard/characters": [
                {
                    "outputs": "player:flatview/action/menu/text_input",
                    "filter": "is_primary_focus(HUD)"
                },
                {
                    "outputs": "gui:overlay/action/menu/text_input",
                    "filter": "is_primary_focus(Overlay)"
                }
            ],
            "/keyboard/key_down": [
                {
                    "outputs": "player:flatview/action/menu/key_down",
                    "filter": "is_primary_focus(HUD)"
                },
                {
                    "outputs": "gui:menu/action/menu/key_down",
                    "filter": "is_primary_focus(Menu)"
                },
                {
                    "outputs": "gui:overlay/action/menu/key_down",
                    "filter": "is_primary_focus(Overlay)"
                }
            ],
            "/keyboard/key_up": [
                {
                    "outputs": "player:flatview/action/menu/key_up",
                    "filter": "is_primary_focus(HUD)"
                },
                {
                    "outputs": "gui:menu/action/menu/key_up",
                    "filter": "is_primary_focus(Menu)"
                },
                {
                    "outputs": "gui:overlay/action/menu/key_up",
                    "filter": "is_primary_focus(Overlay)"
                }
            ],
            "/keyboard/key/escape": [
                {
                    "outputs": "gui:menu/action/menu/open",
                    "filter": "is_primary_focus(Game) && event"
                },
                {
                    "outputs": "gui:menu/action/menu/back",
                    "filter": "is_primary_focus(Menu) && event"
                }
            ],
            "/keyboard/key/space": {
                "outputs": "player:player/action/jump",
                "filter": "is_primary_focus(Game) && event"
            },
            "/keyboard/key/e": [
                {
                    "outputs": "player:pointer/interact/press",
                    "filter": "is_primary_focus(Game)"
                },
                {
                    "outputs": "player:pointer/interact/grab",
                    "filter": "is_primary_focus(Game) && event"
                }
            ],
            "/keyboard/key/q": {
                "outputs": "console:input/action/run_command",
                "filter": "is_primary_focus(Game) && event",
                "set_value": "tray"
            },
            "/keyboard/key/f": {
                "outputs": "player:flashlight/action/flashlight/toggle",
                "filter": "is_primary_focus(Game) && event"
            },
            "/keyboard/key/p": {
                "outputs": "player:flashlight/action/flashlight/grab",
                "filter": "is_primary_focus(Game) && event"
            },
            "/keyboard/key/v": {
                "outputs": "console:input/action/run_command",
                "filter": "is_primary_focus(Game) && event",
                "set_value": "togglesignal player:player/move_noclip"
            }
        }
    },
    "vr:origin": {
        "signal_output": {
            "rotation_rpm": 40
        },
        "signal_bindings": {
            "smooth_rotation": "(vr:controller_left/actions_main_in_rotation.x + vr:controller_right/actions_main_in_rotation.x) * vr:origin/rotation_rpm"
        }
    },
    "vr:left_hand": {
        "signal_bindings": {
            "move_forward": "vr:controller_left/actions_main_in_movement.y",
            "move_right": "vr:controller_left/actions_main_in_movement.x"
        }
    },
    "vr:right_hand": {
        "signal_bindings": {
            "move_forward": "vr:controller_right/actions_main_in_movement.y",
            "move_right": "vr:controller_right/actions_main_in_movement.x"
        },
        "event_bindings": {
            "/interact/grab": {
                "outputs": "player:pointer/interact/grab"
            }
        }
    },
    "vr:controller_left": {
        "event_bindings": {
            "/actions/main/in/press": "vr:left_hand_skeleton/interact/press",
            "/actions/main/in/flashlight": {
                "outputs": "player:flashlight/action/flashlight/toggle",
                "filter": "event"
            },
            "/actions/main/in/grab": "vr:left_hand/interact/grab",
            "/actions/main/in/rotate_left": {
                "outputs": "vr:origin/action/snap_rotate",
                "filter": "event",
                "set_value": -45
            },
            "/actions/main/in/rotate_right": {
                "outputs": "vr:origin/action/snap_rotate",
                "filter": "event",
                "set_value": 45
            }
        }
    },
    "vr:controller_right": {
        "event_bindings": {
            "/actions/main/in/press": "vr:right_hand_skeleton/interact/press",
            "/actions/main/in/flashlight": {
                "outputs": "player:flashlight/action/flashlight/toggle",
                "filter": "event"
            },
            "/actions/main/in/grab": "vr:right_hand/interact/grab",
            "/actions/main/in/rotate_left": {
                "outputs": "vr:origin/action/snap_rotate",
                "filter": "event",
                "set_value": -45
            },
            "/actions/main/in/rotate_right": {
                "outputs": "vr:origin/action/snap_rotate",
                "filter": "event",
                "set_value": 45
            }
        }
    },
    "player:player": {
        "signal_bindings": {
            "move_relative.x": "player:flathead/move_relative.x + vr:left_hand/move_relative.x + vr:right_hand/move_relative.x",
            "move_relative.y": "player:flathead/move_relative.y",
            "move_relative.z": "player:flathead/move_relative.z + vr:left_hand/move_relative.z + vr:right_hand/move_relative.z",
            "move_sprint": "input:keyboard/key_left_shift"
        }
    },
    "player:pointer": {
        "signal_bindings": {
            "interact_rotate": "if_primary_focus(Game, input:keyboard/key_r)"
        }
    },
    "player:flathead": {
        "signal_bindings": {
            "move_forward": "if_primary_focus(Game, input:keyboard/key_w)",
            "move_right": "if_primary_focus(Game, input:keyboard/key_d)",
            "move_left": "if_primary_focus(Game, input:keyboard/key_a)",
            "move_back": "if_primary_focus(Game, input:keyboard/key_s)",
            "move_up": "if_primary_focus(Game, input:keyboard/key_space)",
            "move_down": "if_primary_focus(Game, input:keyboard/key_left_control)"
        }
    }
}
