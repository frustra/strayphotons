        {
            "name": "StrayPhotons VR (vsdbg)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/sp-vk.exe",
            "args": ["--with-validation-layers", "--assets", "../assets/"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/bin",
            "environment": [],
            "console": "integratedTerminal",
            "visualizerFile": "${workspaceFolder}/extra/custom_types.natvis",
            "sourceFileMap": {
                "c:\\buildslave\\steamvr_rel_win64\\build\\src\\public\\vrsdk\\src\\": "${workspaceFolder}/ext/openvr/src"
            }
        },
        {
            "name": "StrayPhotons VR (lldb)",
